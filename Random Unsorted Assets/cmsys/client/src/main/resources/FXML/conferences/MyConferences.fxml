<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="694.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.conferences.MyConferences">
   <children>
      <VBox prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox alignment="CENTER_LEFT" prefHeight="27.0" prefWidth="594.0" VBox.vgrow="NEVER">
               <children>
                  <Button alignment="CENTER" mnemonicParsing="false" onAction="#newConference" prefHeight="25.0" prefWidth="110.0" text="New Conference" />
               </children>
               <padding>
                  <Insets bottom="10.0" />
               </padding>
            </HBox>
            <ScrollPane hbarPolicy="NEVER" VBox.vgrow="ALWAYS">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
               <content>
                  <VBox fx:id="myConferences" prefHeight="135.0" spacing="20.0" />
               </content>
            </ScrollPane>
            <Label styleClass="grey-label" text="Organized by you or participating." />
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="20.0" left="50.0" right="50.0" top="30.0" />
   </padding>
</AnchorPane>
