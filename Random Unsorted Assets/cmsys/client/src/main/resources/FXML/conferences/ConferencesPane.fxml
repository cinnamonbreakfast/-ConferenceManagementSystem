<AnchorPane minHeight="133.0" nodeOrientation="LEFT_TO_RIGHT" prefHeight="135.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.conferences.ConferencesPane">
   <children>
